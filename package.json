{
  "name": "istanbul-instrumenter-loader",
  "version": "3.0.1",
  "description": "Istanbul instrumenter loader for webpack",
  "author": "Kir Belevich (@deepsweet)",
  "main": "dist/cjs.js",
  "files": [
    "dist"
  ],
  "dependencies": {
    "convert-source-map": "^1.7.0",
    "istanbul-lib-instrument": "^1.10.2",
    "loader-utils": "^1.4.0",
    "schema-utils": "^2.7.1"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "license": "MIT",
  "scripts": {
    "start": "npm run build -- -w",
    "build": "cross-env NODE_ENV=production babel src -d dist --ignore 'src/**/*.test.js' --copy-files",
    "clean": "del-cli dist",
    "clean:dist": "del-cli dist",
    "lint": "eslint --cache src test",
    "lint-staged": "lint-staged",
    "prebuild": "npm run clean",
    "prepublish": "npm run build",
    "release": "standard-version",
    "security": "npm audit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --collectCoverageFrom='src/**/*.js' --coverage",
    "travis:coverage": "npm run test:coverage -- --runInBand",
    "travis:lint": "npm run lint",
    "travis:security": "npm run security",
    "travis:test": "npm run test -- --runInBand",
    "appveyor:test": "npm run test",
    "webpack-defaults": "webpack-defaults"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "babel-jest": "^26.3.0",
    "babel-loader": "^7.1.2",
    "cross-env": "^7.0.2",
    "del-cli": "^3.0.1",
    "eslint": "7.7.0",
    "@webpack-contrib/eslint-config-webpack": "^3.0.0",
    "eslint-plugin-import": "^2.22.0",
    "jest": "^26.4.2",
    "lint-staged": "^10.2.13",
    "memory-fs": "^0.5.0",
    "pre-commit": "^1.2.2",
    "standard-version": "^9.0.0",
    "webpack": "^4.44.1",
    "webpack-defaults": "^4.0.0"
  },
  "homepage": "https://github.com/webpack-contrib/istanbul-instrumenter-loader",
  "repository": "https://github.com/webpack-contrib/istanbul-instrumenter-loader.git",
  "keywords": [
    "webpack",
    "loader",
    "istanbul",
    "coverage"
  ],
  "pre-commit": "lint-staged",
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "peerDependencies": {
    "webpack": "^2.0.0 || ^3.0.0 || ^4.0.0"
  }
}
